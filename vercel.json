{
    "version": 2,
    "builds": [
        { "src": "Index.html", "use": "@vercel/static" },
        { "src": "HTML/*.html", "use": "@vercel/static" },
        { "src": "CSS/*.css", "use": "@vercel/static" },
        { "src": "JS/*.js", "use": "@vercel/static" },
        { "src": "images/*", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/$1" }
    ]
}